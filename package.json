{
	"name": "pah",
	"displayName": "phaserAPIHelper",
	"description": "Get API information for a given version of phaser and return the details",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.58.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:pah.apinfo",
		"onCommand:pah.refresh"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "pah.apinfo",
				"category": "Phase API",
				"title": "Get API Information"
			},
			{
				"command": "pah.refresh",
				"category": "Phase API",
				"title": "Refresh Webview"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "rollup -c && tsc -p ./",
		"watch": "concurrently \"rollup -c -w\" \"tsc -watch -p ./\"",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^21.0.1",
		"@rollup/plugin-node-resolve": "^13.0.6",
		"@rollup/plugin-typescript": "^8.3.0",
		"@tsconfig/svelte": "^2.0.1",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/vscode": "^1.58.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"concurrently": "^6.4.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"rollup": "^2.60.1",
		"rollup-plugin-svelte": "^7.1.0",
		"rollup-plugin-terser": "^7.0.2",
		"svelte": "^3.44.2",
		"svelte-check": "^2.2.10",
		"svelte-preprocess": "^4.9.8",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	},
	"dependencies": {
		"node-fetch": "^3.1.0"
	}
}
